<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<!-- Servlets -->
	<servlet>
		<servlet-name>commonServlet</servlet-name>
		<servlet-class>honeycrm.server.CommonServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>commonServlet</servlet-name>
		<url-pattern>/gae/common</url-pattern>
	</servlet-mapping>
	
	<!-- See http://code.google.com/intl/de-DE/appengine/kb/java.html#What_Is_A_Loading_Request -->
    <precompilation-enabled>true</precompilation-enabled>

	<!-- servlet>
		<servlet-name>jUnitHostImpl</servlet-name>
		<servlet-class>com.google.gwt.junit.server.JUnitHostImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>jUnitHostImpl</servlet-name>
		<url-pattern>/gae/junithost/*</url-pattern>
	</servlet-mapping-->

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Gae.html</welcome-file>
	</welcome-file-list>

</web-app>
