<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<g:SplitLayoutPanel>
		<g:west size="300">
			<g:ScrollPanel>
				<g:VerticalPanel>
					<c:CellTable ui:field="list" />
					<c:SimplePager ui:field="pager" />
				</g:VerticalPanel>
			</g:ScrollPanel>
		</g:west>
		<g:center>
			<g:ScrollPanel>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:Button ui:field="createBtn" />
						<g:Button ui:field="deleteBtn" />
						<g:Button ui:field="editBtn" />
						<g:Button ui:field="saveBtn" />
					</g:HorizontalPanel>
					<g:Grid ui:field="grid" visible="false">
						<g:row>
							<g:customCell>
								<g:Label ui:field="contactsName" />
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="name" />
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label ui:field="contactsEmail" />
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="email" />
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label ui:field="contactsPhone" />
							</g:customCell>
							<g:customCell>
								<g:TextBox ui:field="phone" />
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label ui:field="contactsNotes" />
							</g:customCell>
							<g:customCell>
								<g:TextArea ui:field="notes" />
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:VerticalPanel>
			</g:ScrollPanel>
		</g:center>
	</g:SplitLayoutPanel>
</ui:UiBinder>
