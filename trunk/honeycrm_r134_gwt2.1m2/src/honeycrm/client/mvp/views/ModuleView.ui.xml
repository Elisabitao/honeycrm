<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:honeycrm.client.view"
	xmlns:mylist="urn:import:honeycrm.client.mvp.views">
	<g:DockLayoutPanel>
		<g:north size="40">
			<my:ModuleButtonBar ui:field="moduleButtonBar" />
		</g:north>
		<g:center>
			<g:SplitLayoutPanel>
				<g:west size="500">
					<g:ScrollPanel>
						<mylist:ListView ui:field="list" />
					</g:ScrollPanel>
				</g:west>
				<g:center>
					<g:ScrollPanel>
						<mylist:DetailView ui:field="detail" />
					</g:ScrollPanel>
				</g:center>
			</g:SplitLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 