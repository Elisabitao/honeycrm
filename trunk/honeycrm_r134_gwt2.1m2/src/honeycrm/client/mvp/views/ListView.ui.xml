<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<g:DisclosurePanel ui:field="panel" animationEnabled="true" open="true">
		<g:VerticalPanel>
			<g:HorizontalPanel>
				<g:Button ui:field="deleteButton" />
				<g:Button ui:field="addButton" />
			</g:HorizontalPanel>
			<c:SimplePager ui:field="pager" />
			<c:CellTable ui:field="table" />
		</g:VerticalPanel>
	</g:DisclosurePanel>
</ui:UiBinder> 