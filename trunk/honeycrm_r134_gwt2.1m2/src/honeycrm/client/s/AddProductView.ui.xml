<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
		<g:VerticalPanel>
			<g:HorizontalPanel>
				<g:VerticalPanel>
					<g:TextBox ui:field="search" />
					<c:CellTable ui:field="table" />
					<c:SimplePager ui:field="pager" location="CENTER" />
				</g:VerticalPanel>
				<g:VerticalPanel>
					<g:Label ui:field="selectedProductsLbl" />
					<g:TextArea ui:field="selectedProducts" />
				</g:VerticalPanel>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Button ui:field="cancel" />
				<g:Button ui:field="finish" />
			</g:HorizontalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 