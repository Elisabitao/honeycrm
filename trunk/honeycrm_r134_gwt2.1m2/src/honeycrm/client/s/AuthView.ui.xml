<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:ScrollPanel>
		<g:DialogBox ui:field="panel" width="300px">
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label>Username</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="username" >42</g:TextBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label>Password</g:Label>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="password" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:cell>
					</g:cell>
					<g:customCell>
						<g:ListBox ui:field="language" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:cell>
					</g:cell>
					<g:customCell>
						<g:Button ui:field="loginBtn">Login</g:Button>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:DialogBox>
	</g:ScrollPanel>
</ui:UiBinder> 