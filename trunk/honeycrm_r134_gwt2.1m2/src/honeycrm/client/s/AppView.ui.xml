<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.loading {
		margin-left: 100px;
		background-color: #92C1F0;
		font-weight: bold;
		color: white;
	}
	</ui:style>
	<g:FocusPanel ui:field="focus">
		<g:VerticalPanel>
			<g:HorizontalPanel height="30px" >
				<g:Button ui:field="logout" />
				<g:Label ui:field="loading" addStyleNames="{style.loading}" />
			</g:HorizontalPanel>
			<g:ScrollPanel>
				<g:TabLayoutPanel ui:field="panel" barHeight="20" width="1000px" height="700px" />
			</g:ScrollPanel>
		</g:VerticalPanel>
	</g:FocusPanel>
</ui:UiBinder> 